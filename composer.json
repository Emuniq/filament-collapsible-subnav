{
    "name": "emuniq/filament-collapsible-subnav",
    "description": "A Filament v3 & v4 plugin for collapsible sub-navigation sidebar.",
    "license": "MIT",
    "keywords": [
        "filament",
        "laravel",
        "subnavigation",
        "sidebar"
    ],
    "autoload": {
        "psr-4": {
            "Emuniq\\FilamentCollapsibleSubnav\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Emuniq\\FilamentCollapsibleSubnav\\CollapsibleSubnavServiceProvider"
            ]
        }
    },
    "require": {
        "filament/filament": "^3.0|^4.0"
    },
    "scripts": {
        "post-install-cmd": [
            "@php artisan collapsible-subnav:install --ansi || true"
        ],
        "post-update-cmd": [
            "@php artisan collapsible-subnav:install --ansi || true"
        ]
    }
}